@model IEnumerable<Lab4.PlaceInPack>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pack)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Place)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pack.PackId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place.PlaceId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PipId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PipId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PipId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
